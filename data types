#include <stdio.h>

int main()
{
    // integer
    int age = 20;

    // floating-point
    float height = 5.7;

    // double-precision floating-point
    double pi = 3.14159;

    // character
    char grade = 'A';

    printf("Age: %d\n", age);
    printf("Height: %.1f\n", height);
    printf("Pi: %.5lf\n", pi);
    printf("Grade: %c\n", grade);

    return 0;
}
