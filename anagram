#include <stdio.h>
int main()
{
    char a[100],b[100] ;
    int count[256] ={0},i;
    printf("enter first string:");
    scanf("%s", a);
    printf("enter second string:");
    scanf("%s",b);
    for(i = 0; a[i];i++)
    count[a[i]]++;
    for(i = 0;b[i];i++)
    count[b[i]]--;
    
    for (i = 0; i < 256; i++) {
        if (count[i] != 0) {
            printf("Not Anagram");
            return 0;
        }
    }
printf("anagram");
return 0 ;
    
}

